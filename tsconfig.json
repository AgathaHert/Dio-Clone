{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "ES2020",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*", "routes.tsx", "routes.d.ts"],
  "exclude": ["**/node_modules/**"]
}
